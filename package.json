{
  "private": true,
  "sideEffects": false,
  "scripts": {
    "build": "remix build",
    "deploy": "fly deploy --remote-only",
    "dev": "remix dev",
    "lint:check": "eslint .",
    "lint:fix": "eslint . --fix",
    "prettier:check": "prettier --check app",
    "prettier:fix": "prettier --write app",
    "format:check": "yarn lint:check && yarn prettier:check && tsc --noEmit",
    "format:fix": "yarn lint:fix && yarn prettier:fix",
    "migrate": "prisma migrate deploy",
    "migrate:test": "dotenv -e .env.test prisma migrate deploy",
    "prestart": "yarn migrate",
    "start": "remix-serve build",
    "test": "NODE_ENV=test vitest --threads false"
  },
  "prisma": {
    "seed": "ts-node prisma/seed.ts"
  },
